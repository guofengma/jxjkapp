
  <view id="page-my">
    <view class="avatar-box flex--center">
      <view class="avatar-wrap">
        <image src="../assets/images/demo.png" class="avatar"/>
        <text class="username">YK 陈御珂</text>
      </view>
    </view>
    <view class="my-items">
      <block wx:for="{{ myItems }}" wx:for-index="index" wx:for-item="item" wx:key="text">
        <view class="my-item flex--vcenter" bindtap="itemClick" data-wpyitemclick-a="{{ index }}">
          <view class="item-icon-wrap flex-item--none">
            <image class="item-icon" src="{{ item.icon }}"/>
          </view>
          <view class="item-text-wrap flex-item flex--vcenter--sb">
            <text>{{ item.text }}</text>
            <view class="arrow-rt"/>
          </view>
        </view>
      </block>
    </view>
    
  <view class="comp-dialog" style="display: {{ $QrcodeDialog$visible ? 'block' : 'none' }};">
    <view class="dialog__modal" bindtap="$QrcodeDialog$modalClick"/>
    <view class="dialog__inner" style="{{ $QrcodeDialog$innerStyle }}">
      
      <view class="qrcode-dialog flex--col">
        <view class="row flex--vcenter">
          <image class="thumbnail-avatar" src="../assets/images/demo.png"/>
          <text class="username">YK 陈御珂</text>
        </view>
        <view class="flex-item flex--center">
          <view class="qrcode-wrap">
          </view>
        </view>
      </view>
    
    </view>
  </view>

    
  <view class="comp-dialog" style="display: {{ $TelDialog$visible ? 'block' : 'none' }};">
    <view class="dialog__modal" bindtap="$TelDialog$modalClick"/>
    <view class="dialog__inner" style="{{ $TelDialog$innerStyle }}">
      
      <view class="tel-dialog">
        <view class="flex--center title">
          <text>客服电话</text>
        </view>
        <view class="flex--center tel" bindtap="callPhone">
          <text>021-XXXXXXXX</text>
        </view>
      </view>
    
    </view>
  </view>

  </view>