.comp-custom-card{padding:60rpx}.comp-custom-card .card-inner{border-radius:20rpx;box-shadow:0rpx 4rpx 20rpx 0rpx #E3E3E3}.comp-custom-card .card-top{position:relative}.comp-custom-card .card-top::before,.comp-custom-card .card-top::after{content:'';position:absolute;z-index:1;bottom:4rpx;width:50rpx;height:50rpx;background:#F9F9FF}.comp-custom-card .card-top::before{left:0;transform:translate3d(-100%, 50%, 0)}.comp-custom-card .card-top::after{right:0;transform:translate3d(100%, 50%, 0)}.comp-custom-card .card-image{width:100%;height:320rpx;background:blue;border-radius:20rpx 20rpx 0 0}.comp-custom-card .card-title{position:relative;border-bottom:4rpx dashed #D6DAE2;height:128rpx;font-size:15px;line-height:128rpx;text-align:center}.comp-custom-card .card-title::before,.comp-custom-card .card-title::after{content:'';position:absolute;bottom:0;width:50rpx;height:50rpx;border-radius:50%;background:#F9F9FF;box-shadow:0rpx 0rpx 20rpx 0px #E3E3E3 inset}.comp-custom-card .card-title::before{left:0;transform:translate3d(-50%, 50%, 0)}.comp-custom-card .card-title::after{right:0;transform:translate3d(50%, 50%, 0)}.comp-custom-card .card-content{padding:50rpx 40rpx}.comp-custom-card .btn-wrap button{display:block;margin-top:100rpx;border-radius:4rpx;font-size:18px;background:#9190FF;color:#fff;box-shadow:0rpx 10rpx 20rpx 0rpx #8988D4}
